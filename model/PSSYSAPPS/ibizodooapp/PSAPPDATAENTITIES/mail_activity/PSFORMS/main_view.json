    "name" : "onchange_activity_type",
    "getPSDEFIUpdateDetails" : [ {
      "name" : "date_deadline"
    } ],
    "scriptCode" : "data.user_id=context.srfuserid;\r\ndata.user_name=context.srfusername;\r\n//根据活动类型回写数据\r\nvar activityTypeId=data.activity_type_id;\r\nif (activityTypeId) {\r\n    const activityTypeObj={};\r\n    activityTypeObj.id = activityTypeId;\r\n    const app = ibiz.hub.getApp(context.srfappid);\r\n    //计算date_deadline\r\n    const deService = await app.deService.getService(\r\n        context,\r\n        'ibizodooapp.mail_activity_type', \r\n    );\r\n    const result = await  deService.exec('get_date_deadline', context, activityTypeObj);\r\n    if(result.data.actual_deadline){\r\n        data.date_deadline=result.data.actual_deadline;\r\n    }\r\n}\r\nconsole.log('data',data);",
    "customCode" : true
  } ],
